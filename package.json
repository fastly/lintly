{
  "name": "@fastly/lintly",
  "version": "0.0.0",
  "description": "Monorepo for Fastly's linting configuration",
  "scripts": {
    "test": "node_modules/.bin/eslint ./**/*.js ./**/.*.js && node_modules/.bin/prettier --list-different '{*,.*}.js' './*/{*,.*}.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastly/lintly.git"
  },
  "keywords": [
    "eslint",
    "prettier"
  ],
  "author": "Fastly <support@fastly.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastly/lintly/issues"
  },
  "homepage": "https://github.com/fastly/lintly#readme",
  "devDependencies": {
    "@fastly/eslint-config": "file:eslint-config",
    "@fastly/prettier-config": "file:prettier-config",
    "eslint": "^4.12.1",
    "eslint-plugin-ember-suave": "^1.0.0",
    "prettier": "^1.9.2"
  }
}
